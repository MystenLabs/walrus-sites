{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "ESNext"
    ],
    "types": [
      "@cloudflare/workers-types"
    ],
    "baseUrl": "./",
        "paths": {
            "@static/*": ["../common/static/*"],
            "@lib/*": ["../common/lib/*"]
        },
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  },
}
